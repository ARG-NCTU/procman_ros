package procman_lcm;

/* message sent by a procman deputy when a child process writes something to
 * its stdout/stderr fd.
 *
 * Also sent when the deputy itself has something to say.
 */

struct output_t {
    int64_t utime;

    // Which deputy sent this message.
    string deputy_name;

    // id of the child process.  Empty string if the message is generated by
    // the deputy
    string command_id;

    string text;
}
